{
  "name": "mairie-de-saint-jean-cap-ferrat/basic-env - base",

  // "build": {
  //   "dockerfile": "Dockerfile"
  // },

  "image": "mcr.microsoft.com/devcontainers/base:dev-ubuntu",

  "features": {
    "ghcr.io/devcontainers/features/docker-in-docker:2": {
      "moby": true,
      "azureDnsAutoDetection": true,
      "installDockerBuildx": true,
      "installDockerComposeSwitch": true,
      "disableIp6tables": true,
      "version": "latest",
      "dockerDashComposeVersion": "v2"
    },
    "ghcr.io/devcontainers/features/github-cli:1": {
      "installDirectlyFromGitHubRelease": true,
      "version": "latest"
    },
    "ghcr.io/devcontainers/features/common-utils:2": {
      "installZsh": true,
      "configureZshAsDefaultShell": true,
      "installOhMyZsh": true,
      "installOhMyZshConfig": true,
      "upgradePackages": true,
      "username": true,
      "userUid": "automatic",
      "userGid": "automatic"
    },
    "ghcr.io/schlich/devcontainer-features/powerlevel10k:1": {},
    "ghcr.io/devcontainers-extra/features/gitsign-credential-cache:1": {
      "version": "latest"
    },
    "ghcr.io/phorcys420/devcontainer-features/coder:1": {},
    "ghcr.io/devcontainers/features/sshd:1": {
      "version": "latest"
    },
    "ghcr.io/lentzi90/features/tilt:0": {
      "version": "v0.33.17"
    },
    "ghcr.io/wxw-matt/devcontainer-features/command_runner:0": {},
    "ghcr.io/robbert229/devcontainer-features/postgresql-client:1": {
      "version": "13"
    },
    "ghcr.io/devcontainers-extra/features/mkcert:1": {
      "version": "latest"
    },
    "ghcr.io/devcontainers-extra/features/vscode-cli:1": {
      "version": "latest"
    },
    "ghcr.io/devcontainers-extra/features/gitsign:1": {
      "version": "latest"
    },
    "ghcr.io/devcontainers-extra/features/pandoc:1": {
      "version": "latest"
    },
    "ghcr.io/devcontainers-extra/features/pre-commit:2": {
      "version": "latest"
    },
    "ghcr.io/devcontainers-extra/features/prisma:2": {
      "version": "latest"
    },
    "ghcr.io/devcontainers-extra/features/vscode-server:1": {
      "version": "latest"
    },
    "ghcr.io/devcontainers/features/node:1": {
      "version": "lts",
      "installYarnUsingApt": false
    },
    "ghcr.io/devcontainers/features/desktop-lite:1": {
      "version": "latest",
      "noVncVersion": "1.2.0",
      "password": "coder",
      "webPort": "6080",
      "vncPort": "5901"
    }
  },

  "customizations": {
    "vscode": {
      "extensions": [
        "coder.coder-remote",
        "mads-hartmann.bash-ide-vscode",
        "ms-dotnettools.vscode-dotnet-runtime",
        "PDETs-prod.AI-Powered-Authoring",
        "ms-windows-ai-studio.windows-ai-studio",
        "ms-toolsai.vscode-ai",
        "mhutchie.git-graph",
        "donjayamanne.githistory",
        "howardzuo.vscode-git-tags",
        "github.vscode-github-actions",
        "ms-vscode.hexeditor",
        "GitHub.vscode-pull-request-github",
        "bierner.github-markdown-preview",
        "VisualStudioExptTeam.vscodeintellicode",
        "VisualStudioExptTeam.intellicode-api-usage-examples",
        "VisualStudioExptTeam.vscodeintellicode-completions",
        "VisualStudioExptTeam.vscodeintellicode-insiders",
        "ms-vscode.live-server",
        "ms-vsliveshare.vsliveshare",
        "DavidAnson.vscode-markdownlint",
        "prompt-flow.prompt-flow",
        "ms-toolsai.prompty",
        "ms-semantic-kernel.semantic-kernel",
        "VisionAIDevKit.vision-ai-devkit",
        "ms-vscode.vscode-commander",
        "ms-toolsai.vscode-ai-remote",
        "BeardedBear.beardedtheme",
        "GitHub.remotehub",
        "ms-vscode.remote-repositories",
        "vscode-icons-team.vscode-icons",
        "mikestead.dotenv",
        "vivaxy.vscode-conventional-commits",
        "ms-azuretools.vscode-docker",
        "EditorConfig.EditorConfig",
        "PeterSchmalfeldt.explorer-exclude",
        "shyykoserhiy.git-autoconfig",
        "spmeesseman.vscode-taskexplorer",
        "hbenl.vscode-test-explorer",
        "tilt-dev.tiltfile",
        "jock.svg",
        "Gruntfuggly.todo-tree",
        "shardulm94.trailing-spaces",
        "XuangeAha.vsmarketplace-badges",
        "redhat.vscode-yaml",
        "aaron-bond.better-comments",
        "GitHub.codespaces",
        "usernamehw.errorlens",
        "formulahendry.auto-rename-tag",
        "ms-vscode-remote.vscode-remote-extensionpack",
        "oderwat.indent-rainbow",
        "ms-vscode.test-adapter-converter",
        "ms-edgedevtools.vscode-edge-devtools",
        "formulahendry.code-runner",
        "IBM.output-colorizer",
        "lacroixdavid1.vscode-format-context-menu",
        "EverlastEngineering.debug-in-titlebar",
        "DutchIgor.json-viewer",
        "elagil.pre-commit-helper",
        "bierner.markdown-preview-github-styles",
        "berublan.vscode-log-viewer",
        "christian-kohler.path-intellisense",
        "JeronimoEkerdt.color-picker-universal",
        "qcz.text-power-tools",
        "timonwong.shellcheck",
        "antfu.iconify",
        "GrapeCity.gc-excelviewer",
        "foxundermoon.shell-format",
        "mindaro-dev.file-downloader",
        "maciejdems.add-to-gitignore",
        "ms-vscode.vscode-speech",
        "wayou.vscode-todo-highlight",
        "tomoki1207.pdf",
        "adamraichu.docx-viewer",
        "moalamri.inline-fold",
        "ritwickdey.LiveServer",
        "genaiscript.genaiscript-vscode",
        "Jota0222.multi-formatter",
        "adautomendes.yaml2table-preview",
        "ms-vscode-remote.remote-containers"
      ],

      "settings": {
        "json.schemas": [
          {
            "fileMatch": ["*/devcontainer-feature.json"],
            "url": "https://raw.githubusercontent.com/devcontainers/spec/main/schemas/devContainerFeature.schema.json"
          }
        ],
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
          "source.formatDocument": "always"
        }
      }
    }
  },

  "remoteUser": "coder"
}
